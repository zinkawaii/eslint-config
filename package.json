{
  "name": "@zinkawaii/eslint-config",
  "type": "module",
  "version": "0.2.1",
  "packageManager": "pnpm@10.7.0",
  "author": "KazariEX",
  "license": "MIT",
  "repository": "KazariEX/eslint-config",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./index.d.ts",
  "files": [
    "dist",
    "index.d.ts"
  ],
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch",
    "prepack": "pnpm run build",
    "release": "bumpp --no-push -c \"release: v%s\"",
    "eslint": "eslint ."
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.11.0",
    "bumpp": "^10.1.0",
    "eslint": "^9.23.0",
    "tsup": "^8.4.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}